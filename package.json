{
  "name": "smartchat",
  "version": "1.0.0",
  "description": "Command Line Interface for OpenAi ChatGPT",
  "main": "index.js",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "prepare": "husky install && npm run prepare:hooks",
    "prepare:hooks": "npx husky set .husky/commit-msg \"npx commitlint --edit $1\"",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:cov": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage"
  },
  "bin": "./bin/index.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhangqiangzgz/smartChat.git"
  },
  "keywords": [
    "openai",
    "chatgpt",
    "gpt3",
    "gpt4",
    "ml",
    "ai"
  ],
  "files": ["bin", "build"],
  "author": "zhangqiangzgz@hotmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zhangqiangzgz/smartChat/issues"
  },
  "homepage": "https://github.com/zhangqiangzgz/smartChat#readme",
  "dependencies": {
    "@inquirer/prompts": "^1.2.3",
    "@types/gradient-string": "^1.1.2",
    "@types/ini": "^1.3.31",
    "@types/node": "^20.2.5",
    "cac": "^6.7.14",
    "chatgpt": "^5.2.5",
    "cli-markdown": "^3.0.3",
    "conf": "^11.0.1",
    "gradient-string": "^2.0.2",
    "ini": "^4.1.1",
    "kleur": "^4.1.5",
    "log-symbols": "^5.1.0",
    "ora": "^6.3.1",
    "read-pkg-up": "^9.1.0",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@types/jest": "^29.5.2",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "tsup": "^6.7.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
